@(ViewBag.categories as IEnumerable<IdeaCreationManagement.Models.Category>)
   @{
       ViewBag.Title = "AddCategoryToEmp";
    } 
<h2>Przydziel kategorie</h2>
<table class="table">
                
                <tr>
                    <b>Kategorie</b>
                </tr>
                @foreach (var item in ViewBag.categories)
                {
                    <tr>
                        <th>
                            @item.Name
                        </th>
                       
                        <th>
                            @Html.ActionLink("Dodaj kategorie", "AssignCategory", new {id_e=ViewBag.id_e, id_c = item.Id }, new { @class = "btn btn-block btn-default btn-md" })
                        </th>
                    </tr>
                }
</table>

        

